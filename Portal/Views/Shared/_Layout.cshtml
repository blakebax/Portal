    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    @RenderSection("HeaderScripts", false)
</head>
<body>
    <div class="page">
        
        <div id="header">
            <img alt="" src="@Url.Content("/Content/portal_logo.png")" />
        </div>

        <div id="main">
            @RenderBody()
        </div>

        <div id="footer">

        </div>
    </div>
</body>
</html>
